{
	"name": "go-web-template",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"npm:watch:css\" \"npm:watch:ts\" \"npm:watch:vite\" \"npm:watch:templ\"",
		"build": "vite build",
		"watch:test": "npx vitest watch",
		"watch:css": "tailwindcss -i ./css/input.css -o ./dist/output.css --watch",
		"watch:ts": "tsc --watch --noEmit",
		"watch:vite": "vite build --watch",
		"watch:templ": "templ generate --watch --proxy=http://localhost:8080 --cmd='go run .'",
		"templ:generate": "templ generate",
		"lint": "eslint ./ts",
		"lint:fix": "eslint ./ts --fix",
		"test": "vitest run"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@eslint/js": "^9.30.1",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^24.0.0",
		"@typescript-eslint/eslint-plugin": "^8.34.0",
		"@typescript-eslint/parser": "^8.34.0",
		"concurrently": "^9.1.2",
		"esbuild": "^0.25.5",
		"eslint": "^9.30.1",
		"eslint-config-prettier": "^10.1.5",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"lucide-static": "^0.515.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1",
		"vite": "^6.3.5",
		"vitest": "^3.2.3"
	},
	"dependencies": {
		"@tailwindcss/cli": "^4.1.11",
		"htmx.org": "^2.0.4",
		"tailwindcss": "^4.1.11"
	}
}
